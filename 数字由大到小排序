#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int a = 0;
	int b = 0;
	int c = 0;
	scanf("%d%d%d", &a, &b, &c);
	if (a < b)
	{
		int d = 0;
		d = a;
		a = b;
		b = d;
	}
	if (a < c)
	{
		int d = 0;
		d = a;
		a = c;
		c = d;
	}
	if (b < c)
	{
		int d = 0;
		d = b;
		b = c;
		c = d;
	}
	printf("数字由大到小为:%d %d %d", a, b, c);
	return 0;
}
