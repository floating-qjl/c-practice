#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <assert.h>
char* my_strstr(const char* a,const char* b)
{
	assert(a && b);
	char* tmp = (char*)a;
	char* pa = NULL;
	char* pb = NULL;
	if (b == '\0')
		return a;
	while (*tmp)
	{
		pa = tmp;
		pb = (char*)b;
		while ((*pa == *pb) && (*pa && *pb))
		{
			pa++;
			pb++;
		}
		
		if (*pb == '\0')
			return tmp;
		if (*pa == '\0')
			return NULL;
		tmp++;
	}
	return NULL;
}

int main()
{
	char* a = "abbcdefg";
	char* b = "bbc";
	char* ret = my_strstr(a, b);
	if (ret != NULL)
		printf("找到了\n");
	else
		printf("未找到\n");
	return 0;
}
