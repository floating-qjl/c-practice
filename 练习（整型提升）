#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <math.h>

int main()
{
	char a = -1;
	//11111111111111111111111111111111   - 补码
	//11111111  -  一个字节
	//11111111111111111111111111111111   - 整型提升
	signed char b = -1;
	//11111111
	//11111111111111111111111111111111
	unsigned char c = -1;
	//11111111
	//00000000000000000000000011111111
	printf("%d %d %d", a, b, c);
	return 0;
}
