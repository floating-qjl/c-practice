#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <math.h>

int main()
{
	char a = -128;
	//10000000000000000000000010000000
	//11111111111111111111111101111111  -  反码
	//11111111111111111111111110000000  -  补码
	//10000000
	//11111111111111111111111110000000  -  补码（整型提升）
	//无符号数原反补码相同
	printf("%u", a);
	//%u  -   打印十进制的无符号数字
	return 0;
}
