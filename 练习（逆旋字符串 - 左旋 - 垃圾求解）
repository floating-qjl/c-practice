#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
int main()
{
	char a[] = "ABCD";
	int b = 0;
	int n = 0;
	scanf("%d", &n);
	for (b = 0;b < n;b++)
	{
		char c = a[0];
		a[0] = a[1];
		a[1] = a[2];
		a[2] = a[3];
		a[3] = c;
	}
	printf("%s ", a);

	return 0;
}
