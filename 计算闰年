#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int year = 1000;
	int count = 0;
	while (year <= 2000)
	{	
		if (year % 4 == 0 && year % 100 != 0)
		{
			printf("%d\n", year);
			count++;
		}
		else if (year % 400 == 0)
		{
			printf("%d\n", year);
			count++;
		}
		year++;
	}
	printf("%d\n", count);
	return 0;
}
